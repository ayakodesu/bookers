<h1>Editing Book</h1>
<%= form_with model: @book do |f| %>
<div class="field">
    <%= f.label :Title, "Title" %>
    <%= f.text_field :title %>
</div>
<div class="field">
    <%= f.label :body, "Body" %>
    <%= f.text_field :body %>
</div>
<div class="action">
    <%= f.submit 'Update Book' %>

</div>
<% end %>

    <%= link_to "show",book_path(@book.id) %> |
    <%= link_to "Back",books_path(@book.id) %>